{
  "ConnectionStrings": {
    "CronJobManager": "Server=host.docker.internal,1433;Database=CronJobManagerDb;User Id=sa;Password=StrongPassword123;Max Pool Size=100;Encrypt=false"
  },

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },

  "AllowedHosts": "*",

  "Jobs": {
    "ToggleAirJob": {
      "CronValue": "* * * * *",
      "Enabled": true
    },
    "ToggleBoatJob": {
      "CronValue": "*/2 * * * *",
      "Enabled": true
    },
    "ToggleSunJob": {
      "CronValue": "*/3 * * * *",
      "Enabled": true
    },
    "ToggleTruckJob": {
      "CronValue": "*/4 * * * *",
      "Enabled": true
    }
  },

  "Resources": {
    "Resources": [
      {
        "Id": "car_api",
        "Url": "http://client:80/api/"
      }
    ]
  }
}
